<?xml version="1.0"?>
<extension key="com.joineryhq.mosaicohooks" type="module">
  <file>mosaicohooks</file>
  <name>Mosaico Hooks</name>
  <description>Provides hooks civicrm_mosaicoStyleUrlsAlter and civicrm_mosaicoScriptUrlsAlter to alter relevant assets in the Mosaico interface.</description>
  <license>GPL-3.0</license>
  <maintainer>
    <author>Allen Shaw</author>
    <email>allen@joineryhq.com</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://github.com/twomice/com.joineryhq.mosaicohooks</url>
    <url desc="Documentation">https://github.com/twomice/com.joineryhq.mosaicohooks</url>
    <url desc="Support">https://github.com/twomice/com.joineryhq.mosaicohooks/issues</url>
    <url desc="Licensing">http://www.gnu.org/licenses/gpl-3.0.html</url>
  </urls>
  <releaseDate>2021-01-20</releaseDate>
  <version>1.0</version>
  <develStage>beta</develStage>
  <compatibility>
    <ver>5.0</ver>
  </compatibility>
  <comments></comments>
  <classloader>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
  <civix>
    <namespace>CRM/Mosaicohooks</namespace>
  </civix>
</extension>
